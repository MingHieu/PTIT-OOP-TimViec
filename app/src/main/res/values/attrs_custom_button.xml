<resources>
    <declare-styleable name="CustomButton">
        <attr name="custom_button_text" format="string" />
        <attr name="custom_button_color" format="color" />
        <attr name="custom_button_round" format="boolean" />
        <attr name="custom_button_type" format="enum">
            <enum name="primary" value="0" />
            <enum name="secondary" value="1" />
        </attr>
    </declare-styleable>
</resources>